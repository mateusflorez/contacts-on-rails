<div class="row">
  <div class="col-xs-12">
    <h1>New contact</h1>
  </div>

  <div class="col-md-6 col-md-offset-3 col-xs-12">
    <%= form_for [@contact] do |f| %>
      <%= render 'form', f: f %>
      <%= link_to 'back', :back, class: 'btn btn-secondary' %>&nbsp
      <%= f.submit 'Save', class: 'btn btn-primary' %>
    <% end %>
  </div>
</div>
